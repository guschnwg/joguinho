{
  "name": "vanilla",
  "version": "1.0.0",
  "description": "JavaScript example starter project",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "rm -rf ./dist && parcel build index.html --public-url . && cp -R assets ./dist",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "matter-collision-events": "^0.1.7",
    "matter-js": "0.14.2",
    "poly-decomp": "0.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "eslint": "^7.5.0",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.6.1",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0"
  },
  "keywords": [
    "javascript",
    "starter"
  ]
}
